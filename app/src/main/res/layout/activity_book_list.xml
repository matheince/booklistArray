<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#e2d1c3"
    android:orientation="vertical"

    tools:context=".BookListActivity">
    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_above="@+id/book_list_Activity_bottomNavigationBar"
        android:id="@+id/book_list_Activity_framelayout">

    </FrameLayout>


    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/book_list_Activity_bottomNavigationBar"
        android:layout_width="match_parent"

        android:layout_height="wrap_content"


        android:layout_gravity="bottom"
        android:layout_alignParentBottom="true"
        android:background="#fbc2eb"
        app:itemIconTint="@android:color/white"
        app:itemTextColor="@android:color/white"

        app:menu="@menu/bottom_navigation_main" />



</RelativeLayout>